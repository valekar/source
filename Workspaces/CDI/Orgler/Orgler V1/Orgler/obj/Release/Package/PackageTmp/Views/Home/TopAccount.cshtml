@{
    ViewBag.Title = "Top Accounts";
}
<title>Orgler - Top Account</title>
<link rel="stylesheet" href="~/StyleSheets/ui-grid.css" />
@section scripts {

    <script src="@Url.Content("~/App/TopAccount/App.js")"></script>
    <script src="@Url.Content("~/App/TopAccount/Controllers/SearchController.js")"></script>
    <script src="@Url.Content("~/App/TopAccount/Controllers/SearchResultsController.js")"></script>

<script src="@Url.Content("~/App/TopAccount/Controllers/ConstituentDetailsController.js")"></script>
<script src="@Url.Content("~/App/TopAccount/Controllers/ConstMultiDetailsController.js")"></script>
<script src="@Url.Content("~/App/TopAccount/Controllers/InstantiatedControllers/NameInstantiateControllers.js")"></script>
<script src="@Url.Content("~/App/TopAccount/Controllers/InstantiatedControllers/AddressInstantiateControllers.js")"></script>
<script src="@Url.Content("~/App/TopAccount/Controllers/InstantiatedControllers/PhoneInstantiateCtrl.js")"></script>
<script src="@Url.Content("~/App/TopAccount/Controllers/InstantiatedControllers/EmailInstantiateCtrl.js")"></script>

    <script src="@Url.Content("~/App/TopAccount/Services/Service.js")"></script>
    <script src="@Url.Content("~/App/TopAccount/Services/DataService.js")"></script>
    <script src="@Url.Content("~/App/TopAccount/Services/HelperService.js")"></script>
    <script src="@Url.Content("~/App/TopAccount/Directives/Directives.js")"></script>

<script src="@Url.Content("~/App/TopAccount/Services/ConstituentHelper.js")"></script>
<script src="@Url.Content("~/App/TopAccount/Services/ConstituentMultiService.js")"></script>
<script src="@Url.Content("~/App/TopAccount/Services/ConstituentServices.js")"></script>
<script src="@Url.Content("~/App/TopAccount/Services/CRUDService.js")"></script>
<script src="@Url.Content("~/App/TopAccount/Directives/ConstituentDirectives.js")"></script>

}
@section jsCode {
    angular.bootstrap(document.getElementById("topAccountModule"), ['topAccountModule']);
}
<div class="container-fluid" data-ng-app="topAccountModule" id="topAccountModule">
    <br />
    <div class="row">
        <div class="container-fluid">
            <div ui-view="headerContent"></div>
            <div ui-view="pageContent" ></div>
        </div>
    </div>

    <!--Model Po-up-->
    <div class="modal fade" id="topAccountMessageDialogBox" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal">&times;</button>
                    <h4 class="modal-title">{{newAccountModalPopupHeaderText}}</h4>
                </div>
                <div class="modal-body">
                    <p>{{newAccountModalPopupBodyText}}</p>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
    </div>


</div>